  "scripts": {
    "check-versions": "ts-node scripts/check-versions.ts",
    "setup": "echo \"Configuraci贸n completada\"",
    "setup:clean": "echo \"Configuraci贸n limpia completada\"",
    "start:backend": "cd backend && npm start",
    "start:web": "cd frontend/web && npm start",
